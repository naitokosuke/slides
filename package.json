{
  "name": "slides",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "dev": "pnpm run picker dev --open",
    "build": "rimraf dist && pnpm run picker build",
    "build:all": "rimraf dist && pnpm -r run build",
    "build:ci": "rimraf dist && pnpm -r --workspace-concurrency=2 run build",
    "export": "pnpm run picker export",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "fmt": "prettier --write .",
    "picker": "PICKER_EDITOR=code esno scripts/picker.ts",
    "prepare": "git submodule update --init --recursive"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.0",
    "@eslint/config-inspector": "^0.5.1",
    "@iconify-json/carbon": "^1.2.1",
    "@iconify-json/devicon": "^1.2.25",
    "@iconify-json/healthicons": "^1.1.21",
    "@iconify-json/logos": "^1.2.10",
    "@iconify-json/material-icon-theme": "^1.2.29",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/noto": "^1.2.7",
    "@iconify-json/openmoji": "^1.2.0",
    "@iconify-json/simple-icons": "^1.2.4",
    "@iconify-json/skill-icons": "^1.2.0",
    "@iconify-json/twemoji": "^1.1.15",
    "@iconify-json/uim": "^1.1.9",
    "@iconify/json": "^2.2.226",
    "@slidev/cli": "^52.10.0",
    "@slidev/client": "^52.10.0",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "@types/node": "^20.14.10",
    "@types/prompts": "^2.4.9",
    "@types/seedrandom": "^3.0.5",
    "@vueuse/integrations": "^13.9.0",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^0.1.3",
    "esno": "^4.8.0",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.2",
    "markdown-it-magic-link": "^0.1.4",
    "playwright-chromium": "^1.45.1",
    "pnpm": "^9.5.0",
    "prettier": "^3.7.4",
    "prettier-plugin-slidev": "^1.0.5",
    "prompts": "^2.4.2",
    "qrcode": "^1.5.4",
    "rimraf": "^6.0.1",
    "seedrandom": "^3.0.5",
    "taze": "^0.14.2",
    "typescript": "^5.5.3",
    "unocss": "^0.61.3",
    "vue": "^3.5.25",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.0.26"
  }
}
