{
  "name": "slides",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "create": "node scripts/picker.ts create",
    "dev": "PICKER_EDITOR=code node scripts/picker.ts dev --open",
    "build": "rimraf dist && node scripts/picker.ts build",
    "build:all": "rimraf dist && pnpm -r run build",
    "build:ci": "NODE_OPTIONS='--max-old-space-size=4096' node scripts/build-all.ts",
    "export": "node scripts/picker.ts export",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "fmt": "prettier --write .",
    "picker": "PICKER_EDITOR=code node scripts/picker.ts",
    "prepare": "git submodule update --init --recursive"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.0",
    "@eslint/config-inspector": "^0.5.1",
    "@gunshi/docs": "0.27.5",
    "@iconify-json/carbon": "catalog:",
    "@iconify-json/devicon": "catalog:",
    "@iconify-json/healthicons": "catalog:",
    "@iconify-json/logos": "catalog:",
    "@iconify-json/material-icon-theme": "catalog:",
    "@iconify-json/mdi": "catalog:",
    "@iconify-json/noto": "catalog:",
    "@iconify-json/openmoji": "catalog:",
    "@iconify-json/simple-icons": "catalog:",
    "@iconify-json/skill-icons": "catalog:",
    "@iconify-json/twemoji": "catalog:",
    "@iconify-json/uim": "catalog:",
    "@iconify/json": "catalog:",
    "@slidev/cli": "catalog:",
    "@slidev/client": "catalog:",
    "@slidev/theme-default": "catalog:",
    "@slidev/theme-seriph": "catalog:",
    "@types/node": "^20.14.10",
    "@types/prompts": "^2.4.9",
    "@types/seedrandom": "^3.0.5",
    "@vueuse/integrations": "catalog:",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^0.1.3",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.2",
    "markdown-it-magic-link": "^0.1.4",
    "playwright-chromium": "^1.45.1",
    "pnpm": "^9.5.0",
    "prettier": "^3.7.4",
    "prettier-plugin-slidev": "^1.0.5",
    "prompts": "^2.4.2",
    "qrcode": "catalog:",
    "rimraf": "^6.0.1",
    "seedrandom": "^3.0.5",
    "taze": "^0.14.2",
    "typescript": "^5.5.3",
    "unocss": "^0.61.3",
    "vue": "catalog:",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.0.26"
  },
